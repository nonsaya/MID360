<?xml version="1.0"?>
<package format="3">
  <name>glim_extnav_bridge</name>
  <version>0.1.0</version>
  <description>Bridge GLIM odometry to MAVROS /mavros/odometry/in for ArduPilot ExternalNav</description>

  <maintainer email="nonsaya-n@example.com">nonsaya-n</maintainer>
  <license>MIT</license>

  <buildtool_depend>ament_python</buildtool_depend>

  <exec_depend>rclpy</exec_depend>
  <exec_depend>nav_msgs</exec_depend>
  <exec_depend>geometry_msgs</exec_depend>
  <exec_depend>std_msgs</exec_depend>

  <export>
    <build_type>ament_python</build_type>
  </export>
</package>

